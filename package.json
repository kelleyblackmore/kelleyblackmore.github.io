{
  "name": "portfolio-website",
  "private": true,
  "version": "1.0.0",
  "description": "A personal portfolio website built with React and Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prebuild": "node scripts/generate-posts-index.js && node scripts/generate-rss.js && node scripts/generate-sitemap.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:rss": "node scripts/generate-rss.js",
    "generate:posts": "node scripts/generate-posts-index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build"
  },
  "homepage": "https://kelleyblackmore.github.io",
  "dependencies": {
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "react-router-dom": "^6.12.1",
    "react-scripts": "5.0.1",
    "remark-gfm": "^3.0.1",
    "gray-matter": "^4.0.3",
    "buffer": "^6.0.3",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
